<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style>
		.white {
			color: white;
			float: left;
		}
	</ui:style>
	<ui:with type="com.changestuffs.client.place.NameTokens" field="nameTokens"></ui:with>
	<g:HTMLPanel>
		<ul class="eight columns">
			<li>
				<g:InlineHyperlink targetHistoryToken="{nameTokens.getHome}" stylePrimaryName="{style.white} clickable">
					<i class="icon-home" />
					<ui:msg description="home">Home</ui:msg>
				</g:InlineHyperlink>
			</li>
			<li>
				<i class="icon-left entypo icon-login {style.white}" />
				<g:Label ui:field="login" stylePrimaryName="{style.white} clickable">
					<ui:msg description="login">Login</ui:msg>
				</g:Label>
			</li>
		</ul>
	</g:HTMLPanel>

</ui:UiBinder>
