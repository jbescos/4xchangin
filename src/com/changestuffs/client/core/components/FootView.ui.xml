<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:with type="com.changestuffs.client.place.NameTokens" field="nameTokens" />
	<g:HTMLPanel>
		<ul class="eight columns">
			<li>
				<g:InlineHyperlink targetHistoryToken="{nameTokens.getContact}">
					<i class="icon-left entypo icon-paper-plane" />
					<ui:msg description="contact">Contact</ui:msg>
				</g:InlineHyperlink>
			</li>
			<li>
				<a href="http://javanotices.blogspot.com.es/"  target="_blank">
					<i class="icon-left entypo icon-vkontakte" />
					<ui:msg description="blogger">Blogger</ui:msg>
				</a>
			</li>
		</ul>
	</g:HTMLPanel>

</ui:UiBinder>
