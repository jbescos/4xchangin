<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style>
		.border {
			border: 1px solid #D8D8D8;
			border-radius: 4px 4px 4px 4px;
			height: 300px;
		}
	</ui:style>
	<g:HTMLPanel>
		<h4><ui:msg description="images">Upload images</ui:msg></h4>
		<g:FlowPanel ui:field="uploadPanel"/>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="bold">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat 0px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="italic">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -60px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="underline">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -140px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="stroke">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -120px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="subscript">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -600px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="superscript">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -620px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="alignleft">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -460px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="alignmiddle">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -420px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="alignright">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -480px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="orderlist">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -80px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="unorderlist">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -20px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="indentleft">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -400px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="indentright">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -540px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="generatelink">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -500px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="breaklink">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -640px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="insertline">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -360px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="removeformatting">
			<g:upFace>
				<img
					style="width:20px;height:20px;background:url(images/icons.gif) no-repeat -460px -20px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:PushButton>
		<g:ToggleButton stylePrimaryName="columns clickable" ui:field="texthtml">
			<g:upFace>
				<img
					style="transparent width:20px;height:20px;background:url(images/icons.gif) no-repeat -260px 0px;"
					src="changestuffs/clear.cache.gif" />
			</g:upFace>
		</g:ToggleButton>
		<g:PushButton stylePrimaryName="columns clickable" ui:field="youtube">
			<g:upFace>
				<img src="images/youtube.png" />
			</g:upFace>
		</g:PushButton>
		<g:ListBox ui:field="fontlist" stylePrimaryName="columns picker">
			<g:item value="Times New Roman">Times New Roman</g:item>
			<g:item value="Arial">Arial</g:item>
			<g:item value="Courier New">Courier New</g:item>
			<g:item value="Georgia">Georgia</g:item>
			<g:item value="Trebuchet">Trebuchet</g:item>
			<g:item value="Verdana">Verdana</g:item>
		</g:ListBox>
		<g:ListBox ui:field="colorlist" stylePrimaryName="columns picker">
			<g:item value="#FFFFFF">
				<ui:msg description="white">White</ui:msg>
			</g:item>
			<g:item value="000000">
				<ui:msg description="black">Black</ui:msg>
			</g:item>
			<g:item value="red">
				<ui:msg description="red">Red</ui:msg>
			</g:item>
			<g:item value="green">
				<ui:msg description="green">Green</ui:msg>
			</g:item>
			<g:item value="yellow">
				<ui:msg description="yellow">Yellow</ui:msg>
			</g:item>
			<g:item value="blue">
				<ui:msg description="blue">Blue</ui:msg>
			</g:item>
		</g:ListBox>
		<g:RichTextArea ui:field="styleText" stylePrimaryName="row {style.border}" />
	</g:HTMLPanel>
</ui:UiBinder>
